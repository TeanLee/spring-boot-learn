<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.AccountMaper">

<!--    id 要和 mapper 中声明的方法名一致，返回值类型也要和声明的类型保持一致-->
    <select id="getAcct" resultType="com.example.demo.Bean.Account">
        SELECT * FROM `first-database`.account where id=#{id};
    </select>
</mapper>
